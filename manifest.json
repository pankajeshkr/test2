{   "manifest_version": 2,
  "name": "cerch",   
  "description": "This extension shows a Google Image search result for cropped area of the tab",   
  "version": "1.0",
  "browser_action": 
  {     "default_icon": "icon.png",     
        "default_popup": "popup.html"  
  },   
"permissions": ["tabs", "<all_urls>"],
"background": {
    "scripts": ["background.js"]
}}